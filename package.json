{
  "name": "tique-invest-api",
  "version": "1.0.0",
  "description": "API de gest√£o de investimentos",
  "main": "server.js",
  "scripts": {
    "build": "tsc -p .",
    "start": "ts-node -r tsconfig-paths/register ./src/main/index.ts",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register ./src/main/index.ts",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --no-cache",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:clear": "npx jest --clearCache",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wandealves/tique-invest-api.git"
  },
  "keywords": [
    "NodeJs",
    "Investiementos"
  ],
  "author": "Wanderson Alves Rodrigues",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wandealves/tique-invest-api/issues"
  },
  "homepage": "https://github.com/wandealves/tique-invest-api#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "swagger-ui-express": "^4.3.0",
    "tsyringe": "^4.6.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.25",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.2",
    "jest": "^27.2.2",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  }
}
